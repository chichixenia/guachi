   "source": [
    "#判斷輸入的數是否為質數\n",
    "number = int(input())\n",
    "first=0\n",
    "for i in range(1,number):\n",
    "    if number % i == 0:\n",
    "        first += 1\n",
    "if first == 1:\n",
    "    print(\"YES!\")\n",
    "else:\n",
    "    print(\"NO\")\n",
    "#如果是質數:\"YES\"\n",
    "#如果不是質數:\"NO\""
   ]
